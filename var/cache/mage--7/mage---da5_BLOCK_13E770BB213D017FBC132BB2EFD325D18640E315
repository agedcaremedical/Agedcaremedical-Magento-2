                                    itemsCustom: [ [0, 1], [320, 2], [480, 3], [768, 4], [992,4], [1200, 4] ],
                    responsiveRefreshRate: 50,
                 
                                    slideSpeed: 200,
                 
                                    paginationSpeed: 500,
                 
                                    scrollPerPage: true,
                 
                 
                                    stopOnHover: true,
                 
                                    rewindNav: true,
                    rewindSpeed: 600,
                 
                                    pagination: true,
                                    navigation: true,
                    navigationText: false

            });
        });
    });

